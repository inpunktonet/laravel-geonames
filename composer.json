{
    "name": "inpunktonet/laravel-geonames",
    "description": "Populate your database using the GeoNames service.",
    "license": "MIT",
    "keywords": [
        "geonames",
        "geo",
        "country",
        "city",
        "location",
        "address",
        "laravel",
        "database"
    ],
    "homepage": "https://github.com/inpunktonet/laravel-geonames",
    "authors": [
        {
            "name": "Jan Sp√§th",
            "email": "jsp@inpunktonet.de"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-zip": "*",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "nevadskiy/downloader": "^0.4.0",
        "league/csv": "^9.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nevadskiy/laravel-translatable": "^0.8|^0.9"
    },
    "autoload": {
        "psr-4": {
            "InPunktoNET\\Geonames\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InPunktoNET\\Geonames\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InPunktoNET\\Geonames\\GeonamesServiceProvider"
            ]
        }
    },
    "suggest": {
        "nevadskiy/laravel-translatable": "Required to use geonames translations."
    }
}
